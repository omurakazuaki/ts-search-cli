{
  "name": "@kazomur/ts-search-cli",
  "version": "0.1.5",
  "license": "MIT",
  "private": false,
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/omurakazuaki/ts-search-cli.git"
  },
  "bugs": {
    "url": "https://github.com/omurakazuaki/ts-search-cli/issues"
  },
  "homepage": "https://github.com/omurakazuaki/ts-search-cli#readme",
  "bin": {
    "ts-search": "./dist/cli.js"
  },
  "scripts": {
    "gen-version": "echo \"export const version = '$npm_package_version';\" > src/version.ts",
    "prepare": "npm run gen-version",
    "build": "npm run gen-version && tsc && chmod +x dist/cli.js",
    "cli": "ts-node src/cli.ts",
    "test": "jest",
    "coverage": "jest --coverage",
    "lint": "eslint \"{src,.}/**/*.ts\" --fix",
    "format": "prettier --write \"{src,.}/**/*.ts\"",
    "release": "gh release create v$npm_package_version --generate-notes"
  },
  "dependencies": {
    "axios": "^1.13.2",
    "cac": "^6.7.14",
    "cli-table3": "^0.6.5",
    "fastify": "^5.6.2",
    "glob": "^13.0.0",
    "ignore": "^7.0.5",
    "portfinder": "^1.0.38",
    "typescript": "^5.9.3",
    "typescript-language-server": "^4.0.0",
    "vscode-jsonrpc": "^8.2.0",
    "vscode-languageserver-protocol": "^3.17.5",
    "vscode-languageserver-types": "^3.17.5"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^8.48.0",
    "@typescript-eslint/parser": "^8.48.0",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "globals": "^16.5.0",
    "jest": "^30.2.0",
    "prettier": "^3.7.2",
    "ts-jest": "^29.4.5",
    "ts-node": "^10.9.0",
    "typescript-eslint": "^8.48.0"
  },
  "volta": {
    "node": "24.11.1"
  }
}
